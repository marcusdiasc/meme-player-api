{
  "version": 2,
  "env": {
    "API_URL": "@api_url",
    "MONGODB_URI": "@mongodb_uri",
    "API_PORT": "@api_port",
    "FILE_MAX_DURATION": "@file_duration",
    "secret_key": "@secret_keya"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PATCH", "PUT", "DELETE"]
    }
  ]
}
